<%# <% if !tweet.like_users.include?(current_user) %> %>
<% if user_signed_in? && current_user.likes.find_by(tweet_id: tweet.id) %>  
  <%= link_to likes_path(tweet_id: tweet.id), method: :post do %>
    <i class="far fa-heart like-btn" ></i>
  <% end %>
<% else %>
  <%= link_to likes_path(tweet_id: tweet.id), method: :delete do %>
    <i class="fas fa-heart unlike-btn"></i>
  <% end %>
<% end %>
